{
  "name": "spryker/product-offer-availabilities-rest-api",
  "description": "ProductOfferAvailabilitiesRestApi module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/glue-application": "^1.0.0",
    "spryker/glue-application-extension": "^1.0.0",
    "spryker/product-offer-availability-storage": "^0.3.0",
    "spryker/store": "^1.4.0",
    "spryker/symfony": "^3.0.0",
    "spryker/kernel": "^3.33.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
  },
  "scripts": {},
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
